      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "inversedBy": "main_cities"
    },
    "text": {
      "type": "richtext"
    },
    "siteTitle": {
      "type": "string"
    },
    "siteDescription": {
      "type": "text"
    }
  }
