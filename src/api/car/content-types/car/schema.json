{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "className": {
      "type": "string"
    },
    "listCars": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "description": {
      "type": "blocks"
    },
    "isBusiness": {
      "type": "boolean"
    },
    "pricePerKm": {
      "type": "decimal"
    },
    "carImg": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "destinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "inversedBy": "cars"
    }
  }
